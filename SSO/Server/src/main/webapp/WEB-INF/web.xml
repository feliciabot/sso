<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>ServerSendJWT</servlet-name>
        <servlet-class>ServerSendJWT</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendSource</servlet-name>
        <servlet-class>sendSource</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServerSendJWT</servlet-name>
        <url-pattern>/ServerSendJWT</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendSource</servlet-name>
        <url-pattern>/sendSource</url-pattern>
    </servlet-mapping>
</web-app>
