<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>app</servlet-name>
        <servlet-class>app</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetJWT</servlet-name>
        <servlet-class>GetJWT</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSource</servlet-name>
        <servlet-class>GetSource</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>app</servlet-name>
        <url-pattern>/app</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetJWT</servlet-name>
        <url-pattern>/GetJWT</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSource</servlet-name>
        <url-pattern>/GetSource</url-pattern>
    </servlet-mapping>
</web-app>
